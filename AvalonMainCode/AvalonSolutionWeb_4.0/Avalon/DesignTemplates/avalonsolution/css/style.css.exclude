.wizard:before {
    content: "\f0d9";
    font-family: FontAwesome;
    position: absolute;
    display: inline-block;
    top: 24px;
    left: 0;
    font-size: 22px;
    background: #fff;
}
.wizard:after {
    content: "\f0da";
    font-family: FontAwesome;
    position: absolute;
    display: inline-block;
    top: 24px;
    right: -1px;
    font-size: 22px;
    background: #fff;
}

.wizard {
    background:url(../images/checkoutWizard.png) repeat-x 0 30px;
    margin-bottom: 20px;
}
