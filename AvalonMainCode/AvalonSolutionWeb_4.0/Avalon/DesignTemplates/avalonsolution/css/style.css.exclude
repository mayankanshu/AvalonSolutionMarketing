}
#myCarousel .carousel-control {
	position: absolute;
	width: 100%;
	height: 35px;
	color: #FFF;
	text-align: center;
	background: #222;
	-webkit-border-radius: 23px;
	-moz-border-radius: 23px;
	border-radius: 23px;
	opacity: 0.5;
	filter: alpha(opacity=50);
}

/* vertical start*/
.vertical .carousel-inner {
	height: 100%;
}
.vertical .carousel-inner p {
	margin-top: 10px;
    margin-bottom: 15px;
    text-align: center;
}
.vertical .carousel-inner .thumbnail {
  border: none;
  background-color: none;
  margin-bottom: 0px;
  padding-top: 0px;
}
.vertical .carousel-inner .thumbnail img {
  width:120px; 
  height: 90px;
}
.carousel.vertical .item {
	-webkit-transition: 0.6s ease-in-out top;
     -moz-transition: 0.6s ease-in-out top;
      -ms-transition: 0.6s ease-in-out top;
       -o-transition: 0.6s ease-in-out top;
          transition: 0.6s ease-in-out top;
          left: 0;
}
.carousel.vertical .active, .carousel.vertical .next.left, .carousel.vertical .prev.right { 
    top: 0; 
}
.carousel.vertical .next, .carousel.vertical .active.right { 
    top: 100%; 
}
.carousel.vertical .prev, .carousel.vertical .active.left { 
    top: -100%; 
}
#myCarousel .carousel-control.left {
    padding: 0;
}
#myCarousel .carousel-control.right {
    padding: 0;
    margin-top: -50px;
    top:100%!important;
}

.carousel-control.mediaSlider {
    width: 100%;
    padding: 0;
}
.carousel-control.mediaSlider.left i, .carousel-control.mediaSlider.right i {
    padding: 3px 11px;
    font-size: 26px;
    color: #fff;
    background: #888;
    border-radius: 4px;
    width: 100%;
}






