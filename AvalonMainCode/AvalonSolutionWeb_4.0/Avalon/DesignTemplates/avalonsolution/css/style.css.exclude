    position: relative;
    display: inline-block;
    font-size: 14px;
    color: #3B434D;
    pointer-events: none;
}
.wizard .badge {
    margin: 0 0px 12px 0px;
    padding-top: 8px;
    position: relative;
    top: -1px;
    font-size: 24px;
    color: #be5687;
    width: 50px;
    height: 50px;
    border-radius: 25px;
    border: 5px solid #3B434D;
    /*-webkit-box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4);
    box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.4);*/
    background: #ffffff;
}
