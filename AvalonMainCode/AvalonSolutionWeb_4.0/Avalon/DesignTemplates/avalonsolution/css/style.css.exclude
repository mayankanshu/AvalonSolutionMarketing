.leftmenuNew input[type="checkbox"].checkbox {
    display: none;
}
.leftmenuNew input[type="checkbox"].checkbox + label {
    display: inline-block;
    position: relative;
    -webkit-border-radius: 0px;
    -moz-border-radius: 0px;
    border-radius: 0px;
    background: transparent url(../Images/chkLeftMenu.png) no-repeat top;
    width: 11px;
    height: 11px;
    margin-bottom: 0;
    margin-right: 5px;
}
.leftmenuNew input[type="checkbox"].checkbox:checked + label {
    background: transparent url(../Images/chkLeftMenu.png) no-repeat bottom;
}
.leftmenuNew input[type="checkbox"].checkbox:disabled + label {
    opacity: 0.5;
}
